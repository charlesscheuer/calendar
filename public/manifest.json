{
  "manifest_version": 2,
  "name": "NextCall.xyz",
  "description" : "One click join any meeting",
  "version": "0.0.0.1",
  "browser_action": {
    "default_title": "NextCall",
    "default_popup": "index.html"
  },
  "content_scripts": [ {
    "matches": ["https://nextcall.xyz/"],
    "css": ["/static/css/main.css"],
    "js": ["/static/js/main.js"]
  } ],
  "permissions": [
    "notifications",
    "storage"
  ]
}
