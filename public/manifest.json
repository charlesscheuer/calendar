{
  "manifest_version": 2,
  "name": "calendar app",
  "version": "0.1",
  "browser_action": {
    "default_title": "My extension",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ],
  "permissions": ["notifications", "storage"]
}
