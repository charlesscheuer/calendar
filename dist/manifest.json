{
  "manifest_version": 2,
  "name": "calendar",
  "description": "quickly join your next meeting",
  "version": "0.0.0.1",
  "browser_action": {
    "default_title": "Calendar",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
  "permissions": ["notifications", "storage"]
}
